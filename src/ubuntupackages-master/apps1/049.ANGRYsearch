#!/bin/bash

#ANGRYsearch
sudo apt-fast -y install python3-pyqt5
cd $HOME
wget https://github.com/DoTheEvo/ANGRYsearch/archive/v0.9.9.tar.gz
mkdir $HOME/ANGRYsearch
tar zxvf $HOME/v0.9.9.tar.gz -C $HOME/ANGRYsearch
rm $HOME/v0.9.9.tar.gz
cd $HOME/ANGRYsearch/ANGRYsearch-0.9.9
chmod +x install.sh
sudo ./install.sh
rm -rf $HOME/ANGRYsearch